{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "noEmit": true,
    "strict": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "typeRoots": ["./types", "./node_modules/@types"],
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
